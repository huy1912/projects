    <query name="getRecentHaemodialysisChart">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 and dialysis.patientId = :patientId order by dialysis.haemodialysisChartId desc]]>
     </query>
     <query name="getHDUseDialyserNo">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 and dialysis.patientId = :patientId and dialyserNo = :dialyserNo and haemodialysisChartId != :haemodialysisChartId order by dialysis.haemodialysisChartId desc]]>
     </query>
     <query name="getOtherHDUseDialyserNo">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 and dialysis.patientId != :patientId and dialyserNo = :dialyserNo order by dialysis.haemodialysisChartId desc]]>
     </query>
     <query name="getPreviousHaemodialysisChart">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 
     	and dialysis.patientId = :patientId 
     	and dialysis.haemodialysisChartId < :haemodialysisChartId
     	order by dialysis.haemodialysisChartId desc]]>
     </query>
     <query name="getPreviousHaemodialysisChartByDay">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 
     	and dialysis.patientId = :patientId 
     	and dialysis.createdDate < :today
     	order by dialysis.haemodialysisChartId desc]]>
     </query>
     <query name="getTodayHaemodialysisChart">
     	<![CDATA[select dialysis from HaemodialysisChart dialysis where dialysis.entityStatus = 1 
     	and dialysis.patientId = :patientId 
     	and dialysis.createdDate >= :today
     	order by dialysis.haemodialysisChartId desc]]>
     </query>     
